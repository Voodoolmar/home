{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/bin/www",
			"args": [],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": "server",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy", "--debug"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"port": 5858,
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": null
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"address": "localhost",
			"restart": false,
			"sourceMaps": false,
			"outDir": null,
			"localRoot": "${workspaceRoot}",
			"remoteRoot": null
		},
		{
			"name": "Attach to Process",
			"type": "node",
			"request": "attach",
			"processId": "${command.PickProcess}",
			"port": 5858,
			"sourceMaps": false,
			"outDir": null
		}
	]
}